  pod 'Masonry', '~> 1.0.1'
  pod 'UICountingLabel'
  pod 'SDCycleScrollView','~> 1.65'
  pod 'SDWebImage', '~> 3.8.1'
end

target 'Izzqg’ do
  pods
end

target 'Izzqg Test’ do
pods
end

target 'Izzqg Dev’ do
  pods
end


